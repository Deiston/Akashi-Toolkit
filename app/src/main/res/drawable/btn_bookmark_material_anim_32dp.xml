<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@+id/checked"
        android:state_checked="true"
        android:drawable="@drawable/ic_bookmark_checked_32dp"/>
    <item
        android:id="@+id/unchecked"
        android:drawable="@drawable/ic_bookmark_unchecked_32dp"/>
    <transition
        android:fromId="@+id/unchecked"
        android:toId="@+id/checked"
        android:drawable="@drawable/ic_bookmark_unchecked_to_checked_animation_32dp"/>
    <transition
        android:fromId="@+id/checked"
        android:toId="@+id/unchecked"
        android:drawable="@drawable/ic_bookmark_checked_to_unchecked_animation_32dp"/>
</animated-selector>